<recipe>
  <title>Banana Cupcakes With Chocolate Frosting Recipe by Tasty</title>
  <url>https://tasty.co/recipe/banana-cupcakes-with-chocolate-frosting</url>
  <type>compilation</type>
  <comp_url>https://tasty.co/compilation/4-healthier-banana-swap-outs</comp_url>
  <ingredients>
    <ingredient num="0">1 ; banana </ingredient>
    <ingredient num="1">1 box ; yellow cake mix </ingredient>
    <ingredient num="2">1 cup ; water ; (236 mL) </ingredient>
    <ingredient num="3">3 ; eggs </ingredient>
    <ingredient num="4">2 ; bananas </ingredient>
    <ingredient num="5">2 cups ; cocoa powder ; (240 g) </ingredient>
    <ingredient num="6">½ cup ; honey ; (170 g) </ingredient>
    <ingredient num="7">½ teaspoon ; vanilla </ingredient>
    <ingredient num="8">1 teaspoon ; salt </ingredient>
  </ingredients>
  <categories>
    <category num="0">American</category>
    <category num="1">Healthy</category>
