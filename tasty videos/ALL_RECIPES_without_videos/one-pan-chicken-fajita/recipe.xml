  <ingredients>
    <ingredient num="0">½ ; green bell pepper, sliced </ingredient>
    <ingredient num="1">red bell pepper, sliced </ingredient>
    <ingredient num="2">¼ ; onion </ingredient>
    <ingredient num="3">olive oil </ingredient>
    <ingredient num="4">salt, to taste </ingredient>
    <ingredient num="5">pepper, to taste </ingredient>
    <ingredient num="6">1 ; chicken breast </ingredient>
    <ingredient num="7">1 ; parchment paper </ingredient>
    <ingredient num="8">fajita seasoning </ingredient>
    <ingredient num="9">½ tablespoon ; chilli powder </ingredient>
    <ingredient num="10">¼ teaspoon ; garlic powder </ingredient>
    <ingredient num="11">¼ teaspoon ; cumin </ingredient>
    <ingredient num="12">¼ teaspoon ; paprika </ingredient>
    <ingredient num="13">¼ teaspoon ; salt </ingredient>
    <ingredient num="14">¼ teaspoon ; ground black pepper </ingredient>
  </ingredients>
