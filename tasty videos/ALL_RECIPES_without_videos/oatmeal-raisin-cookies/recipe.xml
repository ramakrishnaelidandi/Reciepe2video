  <ingredients>
    <ingredient num="0">¼ cup ; maple syrup ; (55 g) </ingredient>
    <ingredient num="1">1 ; egg </ingredient>
    <ingredient num="2">1 teaspoon ; vanilla extract </ingredient>
    <ingredient num="3">1 cup ; whole wheat flour ; (115 g) </ingredient>
    <ingredient num="4">1 teaspoon ; ground cinnamon </ingredient>
    <ingredient num="5">½ teaspoon ; baking soda </ingredient>
    <ingredient num="6">1 pinch ; salt </ingredient>
    <ingredient num="7">1 cup ; old-fashioned oat ; (100 g) </ingredient>
    <ingredient num="8">1 cup ; raisin ; (150 g) </ingredient>
    <ingredient num="9">¼ cup ; unsweetened applesauce ; (65 mL) </ingredient>
    <ingredient num="10">½ ; avocado, mashed </ingredient>
    <ingredient num="11">½ ; banana, mashed </ingredient>
    <ingredient num="12">¼ cup ; olive oil ; (60 mL) </ingredient>
  </ingredients>
