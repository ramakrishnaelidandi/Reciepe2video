  <ingredients>
    <ingredient num="0">4 cups ; cauliflower rice ; (400 g) </ingredient>
    <ingredient num="1">2 cups ; broccoli floret ; (300 g) </ingredient>
    <ingredient num="2">2 cups ; chicken, cooked, cubed ; (300 g) </ingredient>
    <ingredient num="3">½ cup ; shredded mozzarella cheese ; (50 g) </ingredient>
    <ingredient num="4">1 cup ; shredded cheddar cheese, divided </ingredient>
    <ingredient num="5">1 teaspoon ; garlic powder </ingredient>
    <ingredient num="6">1 teaspoon ; onion powder </ingredient>
    <ingredient num="7">salt, to taste </ingredient>
    <ingredient num="8">fresh parsley, optional, to serve </ingredient>
  </ingredients>
