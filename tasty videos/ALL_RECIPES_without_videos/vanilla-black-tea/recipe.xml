  <ingredients>
    <ingredient num="0">1 cup ; black tea ; (240 mL) </ingredient>
    <ingredient num="1">½ cup ; almond milk ; (120 mL) </ingredient>
    <ingredient num="2">1 tablespoon ; vanilla protein powder </ingredient>
    <ingredient num="3">½ cup ; plain yogurt ; (120 g) </ingredient>
    <ingredient num="4">1 ; banana, sliced </ingredient>
    <ingredient num="5">1 cup ; blueberry ; (100 g) </ingredient>
    <ingredient num="6">2 cups ; ice ; (280 g) </ingredient>
    <ingredient num="7">sliced almond, for serving </ingredient>
  </ingredients>
  <categories>
    <category num="0">Healthy</category>
    <category num="1">Vegetarian</category>
    <category num="2">Breakfast</category>
  </categories>
