  <ingredients>
    <ingredient num="0">1 cup ; water ; (235 mL) </ingredient>
    <ingredient num="1">1 ½ cups ; orange juice ; (355 mL) </ingredient>
    <ingredient num="2">1 cup ; quinoa ; (170 g) </ingredient>
    <ingredient num="3">strawberry </ingredient>
    <ingredient num="4">blueberry </ingredient>
    <ingredient num="5">mango, cubed </ingredient>
    <ingredient num="6">fresh basil, chopped, for garnish </ingredient>
    <ingredient num="7">½ cup ; lime juice ; (120 mL) </ingredient>
    <ingredient num="8">1 tablespoon ; honey </ingredient>
  </ingredients>
