  <ingredients>
    <ingredient num="0">2 tablespoons ; canola oil </ingredient>
    <ingredient num="1">2 ; boneless, skinless chicken breasts, diced </ingredient>
    <ingredient num="2">½ teaspoon ; salt </ingredient>
    <ingredient num="3">½ teaspoon ; pepper </ingredient>
    <ingredient num="4">2 cloves ; garlic, chopped </ingredient>
    <ingredient num="5">2 cups ; broccoli floret ; (500 g) </ingredient>
    <ingredient num="6">2 cups ; milk ; (470 mL) </ingredient>
    <ingredient num="7">1 cup ; shredded parmesan cheese ; (110 g) </ingredient>
    <ingredient num="8">¼ cup ; fresh parsley, chopped ; (10 g) </ingredient>
    <ingredient num="9">4 strips ; bacon, cooked, crumbled </ingredient>
    <ingredient num="10">½ lb ; spaghetti, cooked ; (225 g) </ingredient>
  </ingredients>
  <categories>
    <category num="0">Italian</category>
