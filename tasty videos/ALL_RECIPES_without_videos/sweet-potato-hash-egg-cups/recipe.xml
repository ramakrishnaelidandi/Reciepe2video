<recipe>
  <title>Sweet Potato Hash Egg Cups Recipe by Tasty</title>
  <url>https://tasty.co/recipe/sweet-potato-hash-egg-cups</url>
  <type>single</type>
  <ingredients>
    <ingredient num="0">½ ; sweet potato, peeled and shredded </ingredient>
    <ingredient num="1">olive oil, drizzle </ingredient>
    <ingredient num="2">salt, to taste </ingredient>
    <ingredient num="3">pepper, to taste </ingredient>
    <ingredient num="4">6 ; large eggs </ingredient>
    <ingredient num="5">tomato, diced </ingredient>
    <ingredient num="6">fresh basil, finely chopped </ingredient>
    <ingredient num="7">garlic, minced </ingredient>
    <ingredient num="8">fresh spinach, finely chopped </ingredient>
    <ingredient num="9">bell pepper, diced </ingredient>
    <ingredient num="10">mushroom, chopped </ingredient>
    <ingredient num="11">pepper, to taste </ingredient>
    <ingredient num="12">low-fat mozzarella, optional </ingredient>
    <ingredient num="13">green onion, to taste </ingredient>
  </ingredients>
  <categories>
    <category num="0">American</category>
    <category num="1">Healthy</category>
    <category num="2">Low-Carb</category>
