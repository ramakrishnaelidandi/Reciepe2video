<recipe>
  <title>Loaded Baked Potato Soup Recipe by Tasty</title>
  <url>https://tasty.co/recipe/loaded-baked-potato-soup</url>
  <type>compilation</type>
  <comp_url>https://tasty.co/compilation/5-soups-to-warm-your-soul</comp_url>
  <ingredients>
    <ingredient num="0">4 lb ; russet potato, washed ; (1.8 g) </ingredient>
    <ingredient num="1">1 tablespoon ; olive oil </ingredient>
    <ingredient num="2">1 teaspoon ; salt </ingredient>
    <ingredient num="3">½ teaspoon ; pepper </ingredient>
    <ingredient num="4">1 ; onion, diced </ingredient>
    <ingredient num="5">3 cloves ; garlic </ingredient>
    <ingredient num="6">3 tablespoons ; butter </ingredient>
    <ingredient num="7">¼ cup ; flour ; (30 g) </ingredient>
    <ingredient num="8">2 ½ cups ; chicken broth ; (590 mL) </ingredient>
    <ingredient num="9">8 oz ; cream cheese ; (225 g) </ingredient>
    <ingredient num="10">shredded cheese </ingredient>
    <ingredient num="11">bacon, diced </ingredient>
    <ingredient num="12">fresh chives, chopped </ingredient>
  </ingredients>
  <categories>
    <category num="0">American</category>
    <category num="1">Comfort Food</category>
    <category num="2">Dinner</category>
    <category num="3">Lunch</category>
    <category num="4">Date Night</category>
    <category num="5">Weeknight</category>
  </categories>
  <video>
    <low>https://vid.tasty.co/output/65038/hls24_1512080123.m3u8</low>
    <high>https://vid.tasty.co/output/65038/landscape_720/1512080123</high>
  </video>
