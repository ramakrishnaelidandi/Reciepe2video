  <ingredients>
    <ingredient num="0">1 lb ; silken tofu ; (455 g) </ingredient>
    <ingredient num="1">¼ cup ; maple syrup ; (55 g) </ingredient>
    <ingredient num="2">½ ; banana, sliced </ingredient>
    <ingredient num="3">1 ; kiwi, peeled and diced </ingredient>
    <ingredient num="4">½ ; mango, peeled and diced </ingredient>
    <ingredient num="5">1 cup ; fresh spinach, packed ; (40 g) </ingredient>
    <ingredient num="6">½ cup ; soy milk, or other milk substitute ; (120 mL) </ingredient>
  </ingredients>
