  <ingredients>
    <ingredient num="0">4 tablespoons ; flour </ingredient>
    <ingredient num="1">3 tablespoons ; sugar </ingredient>
    <ingredient num="2">½ teaspoon ; baking powder </ingredient>
    <ingredient num="3">2 ; strawberries, diced </ingredient>
    <ingredient num="4">3 tablespoons ; milk </ingredient>
    <ingredient num="5">1 tablespoon ; oil </ingredient>
    <ingredient num="6">1 teaspoon ; vanilla extract </ingredient>
    <ingredient num="7">1 ; strawberry, for topping, optional </ingredient>
    <ingredient num="8">whipped topping, for topping, optional </ingredient>
  </ingredients>
  <categories>
    <category num="0">American</category>
