  <ingredients>
    <ingredient num="0">¾ lb ; ground beef ; (340 g) </ingredient>
    <ingredient num="1">½ tablespoon ; dried parsley </ingredient>
    <ingredient num="2">½ tablespoon ; cumin </ingredient>
    <ingredient num="3">½ tablespoon ; dried oregano </ingredient>
    <ingredient num="4">½ tablespoon ; dried basil </ingredient>
    <ingredient num="5">½ tablespoon ; garlic powder </ingredient>
    <ingredient num="6">½ tablespoon ; dried thyme </ingredient>
    <ingredient num="7">1 tablespoon ; red pepper, diced, cooked </ingredient>
    <ingredient num="8">1 slice ; pepper jack cheese, quartered </ingredient>
    <ingredient num="9">1 tablespoon ; green pepper, diced, cooked </ingredient>
    <ingredient num="10">1 ; butter-toasted sesame bun </ingredient>
    <ingredient num="11">1 tablespoon ; guacamole </ingredient>
    <ingredient num="12">1 tablespoon ; salsa </ingredient>
    <ingredient num="13">1 tablespoon ; sour cream </ingredient>
