  <ingredients>
    <ingredient num="0">1 tablespoon ; dried oregano </ingredient>
    <ingredient num="1">1 tablespoon ; dried basil </ingredient>
    <ingredient num="2">1 tablespoon ; dried parsley </ingredient>
    <ingredient num="3">2 teaspoons ; dried thyme </ingredient>
    <ingredient num="4">2 teaspoons ; dried rosemary </ingredient>
    <ingredient num="5">2 teaspoons ; garlic powder </ingredient>
    <ingredient num="6">1 teaspoon ; salt </ingredient>
  </ingredients>
  <categories>
    <category num="0">Italian</category>
    <category num="1">Vegetarian</category>
  </categories>
