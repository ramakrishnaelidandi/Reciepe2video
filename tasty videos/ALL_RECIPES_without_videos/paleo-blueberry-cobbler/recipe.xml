  <ingredients>
    <ingredient num="0">1 cup ; almond flour ; (100 g) </ingredient>
    <ingredient num="1">½ cup ; sliced almond ; (35 g) </ingredient>
    <ingredient num="2">¼ cup ; virgin coconut oil, melted ; (60 mL) </ingredient>
    <ingredient num="3">2 tablespoons ; organic maple syrup, unprocessed </ingredient>
    <ingredient num="4">½ teaspoon ; additive-free cinnamon </ingredient>
    <ingredient num="5">1 pinch ; nutmeg </ingredient>
    <ingredient num="6">1 pinch ; sea salt </ingredient>
    <ingredient num="7">24 oz ; fresh blueberry ; (680 g) </ingredient>
    <ingredient num="8">1 ; lemon, juiced </ingredient>
  </ingredients>
