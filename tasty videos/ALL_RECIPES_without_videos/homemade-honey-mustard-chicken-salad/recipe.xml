  <ingredients>
    <ingredient num="0">1 tablespoon ; mustard </ingredient>
    <ingredient num="1">1 tablespoon ; honey </ingredient>
    <ingredient num="2">½ tablespoon ; olive oil </ingredient>
    <ingredient num="3">1 squeeze ; lemon juice </ingredient>
    <ingredient num="4">¼ teaspoon ; salt </ingredient>
    <ingredient num="5">¼ teaspoon ; pepper </ingredient>
    <ingredient num="6">¼ teaspoon ; garlic powder </ingredient>
    <ingredient num="7">2 cups ; romaine lettuce, chopped ; (140 g) </ingredient>
    <ingredient num="8">¼ cup ; red onion, sliced ; (35 g) </ingredient>
    <ingredient num="9">¼ cup ; cherry tomato, halved ; (50 g) </ingredient>
    <ingredient num="10">¼ cup ; avocado, diced ; (35 g) </ingredient>
    <ingredient num="11">1 ; rotisserie chicken breast, sliced </ingredient>
    <ingredient num="12">1 ; egg, hard boiled and sliced </ingredient>
  </ingredients>
