<recipe>
  <title>“Ranch” Roasted Chickpeas Recipe by Tasty</title>
  <url>https://tasty.co/recipe/ranch-roasted-chickpeas</url>
  <type>compilation</type>
  <comp_url>https://tasty.co/compilation/roasted-chickpeas-4-ways</comp_url>
  <ingredients>
    <ingredient num="0">15 oz ; chickpeas, 1 can, drained and rinsed ; (400 g) </ingredient>
    <ingredient num="1">1 tablespoon ; olive oil </ingredient>
    <ingredient num="2">½ teaspoon ; dried thyme </ingredient>
    <ingredient num="3">½ teaspoon ; dried parsley </ingredient>
    <ingredient num="4">½ teaspoon ; onion powder </ingredient>
    <ingredient num="5">1 teaspoon ; dried dill </ingredient>
    <ingredient num="6">½ teaspoon ; garlic powder </ingredient>
    <ingredient num="7">½ teaspoon ; pepper </ingredient>
  </ingredients>
  <categories>
    <category num="0">Gluten-Free</category>
    <category num="1">Healthy</category>
    <category num="2">Vegan</category>
