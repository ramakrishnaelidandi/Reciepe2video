<recipe>
  <title>Chocolate Avocado Brownies Recipe by Tasty</title>
  <url>https://tasty.co/recipe/chocolate-avocado-brownies</url>
  <type>compilation</type>
  <comp_url>https://tasty.co/compilation/avocado-desserts-4-ways</comp_url>
  <ingredients>
    <ingredient num="0">1 ; ripe avocado, cubed </ingredient>
    <ingredient num="1">2 ; eggs </ingredient>
    <ingredient num="2">½ cup ; honey ; (170 g) </ingredient>
    <ingredient num="3">1 teaspoon ; vanilla extract </ingredient>
    <ingredient num="4">⅔ cup ; whole wheat flour ; (75 g) </ingredient>
    <ingredient num="5">¼ cup ; cocoa powder ; (30 g) </ingredient>
    <ingredient num="6">1 teaspoon ; baking powder </ingredient>
  </ingredients>
  <categories>
    <category num="0">Dairy-Free</category>
    <category num="1">Healthy</category>
    <category num="2">Vegetarian</category>
    <category num="3">Desserts</category>
    <category num="4">Weeknight</category>
  </categories>
