  <ingredients>
    <ingredient num="0">2 cups ; chickpeas, drained and rinsed ; (330 g) </ingredient>
    <ingredient num="1">1 ½ teaspoons ; salt </ingredient>
    <ingredient num="2">½ teaspoon ; pepper </ingredient>
    <ingredient num="3">½ teaspoon ; cumin </ingredient>
    <ingredient num="4">¼ cup ; tahini paste ; (60 g) </ingredient>
    <ingredient num="5">2 tablespoons ; lemon juice </ingredient>
    <ingredient num="6">2 tablespoons ; olive oil </ingredient>
    <ingredient num="7">1 package ; whole wheat tortillas </ingredient>
    <ingredient num="8">1 ; red bell pepper, sliced </ingredient>
    <ingredient num="9">2 ; carrots, sliced </ingredient>
    <ingredient num="10">1 ; yellow bell pepper, sliced </ingredient>
    <ingredient num="11">1 ; cucumber, sliced </ingredient>
    <ingredient num="12">¼ ; shredded purple cabbage </ingredient>
  </ingredients>
