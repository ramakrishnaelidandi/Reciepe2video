<recipe>
  <title>Quinoa Seed Crackers Recipe by Tasty</title>
  <url>https://tasty.co/recipe/quinoa-seed-crackers</url>
  <type>compilation</type>
  <comp_url>https://tasty.co/compilation/9-healthier-workout-snacks</comp_url>
  <ingredients>
    <ingredient num="0">⅓ cup ; chia seed ; (55 g) </ingredient>
    <ingredient num="1">¾ cup ; water ; (180 mL) </ingredient>
    <ingredient num="2">¾ cup ; quinoa, cooked ; (125 g) </ingredient>
    <ingredient num="3">⅓ cup ; pumpkin seeds ; (45 g) </ingredient>
    <ingredient num="4">⅓ cup ; sesame seed ; (50 g) </ingredient>
    <ingredient num="5">salt, to taste </ingredient>
    <ingredient num="6">pepper, to taste </ingredient>
    <ingredient num="7">hummus, to serve </ingredient>
  </ingredients>
  <categories>
    <category num="0">American</category>
    <category num="1">Healthy</category>
    <category num="2">Vegan</category>
    <category num="3">Vegetarian</category>
    <category num="4">Snacks</category>
  </categories>
  <video>
    <low>https://vid.tasty.co/output/65579/hls24_1512413303.m3u8</low>
    <high>https://vid.tasty.co/output/65579/landscape_720/1512413303</high>
