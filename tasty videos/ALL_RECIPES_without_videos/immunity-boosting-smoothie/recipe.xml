  <ingredients>
    <ingredient num="0">4 ; clementines, peeled </ingredient>
    <ingredient num="1">1 ; ripe banana, sliced </ingredient>
    <ingredient num="2">1 ; skinny carrot, 6 inch (15 cm), peeled and shaved </ingredient>
    <ingredient num="3">1 piece ; fresh ginger, 1 inch (2 cm) peeled and minced </ingredient>
    <ingredient num="4">¼ cup ; plain yogurt ; (60 g) </ingredient>
    <ingredient num="5">1 tablespoon ; honey </ingredient>
    <ingredient num="6">salt, to taste </ingredient>
    <ingredient num="7">1 ½ cups ; ice ; (190 g) </ingredient>
  </ingredients>
  <categories>
    <category num="0">Gluten-Free</category>
