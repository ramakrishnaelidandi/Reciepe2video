<recipe>
  <title>Chicken Tikka Goujons Recipe by Tasty</title>
  <url>https://tasty.co/recipe/chicken-tikka-goujons</url>
  <type>single</type>
  <ingredients>
    <ingredient num="0">2 ; chicken breasts </ingredient>
    <ingredient num="1">⅓ cup ; plain flour ; (50 g) </ingredient>
    <ingredient num="2">1 cup ; panko breadcrumbs ; (50 g) </ingredient>
    <ingredient num="3">2 ; eggs, beaten </ingredient>
    <ingredient num="4">mango chutney, to serve </ingredient>
    <ingredient num="5">4 tablespoons ; greek yogurt </ingredient>
    <ingredient num="6">1 teaspoon ; chili powder </ingredient>
    <ingredient num="7">2 teaspoons ; garam masala </ingredient>
    <ingredient num="8">1 teaspoon ; turmeric </ingredient>
    <ingredient num="9">1 teaspoon ; fresh ginger, crushed </ingredient>
    <ingredient num="10">1 teaspoon ; garlic, crushed </ingredient>
    <ingredient num="11">salt, to taste </ingredient>
    <ingredient num="12">pepper, to taste </ingredient>
  </ingredients>
  <categories>
    <category num="0">American</category>
    <category num="1">Indian</category>
    <category num="2">Comfort Food</category>
    <category num="3">Appetizers</category>
    <category num="4">Game Day</category>
    <category num="5">Casual Party</category>
    <category num="6">Fusion</category>
  </categories>
