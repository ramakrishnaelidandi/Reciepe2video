  <ingredients>
    <ingredient num="0">2 tablespoons ; olive oil </ingredient>
    <ingredient num="1">3 cloves ; garlic, chopped </ingredient>
    <ingredient num="2">2 ; chicken breasts, thinly sliced </ingredient>
    <ingredient num="3">2 cups ; asparagus, chopped ; (250 g) </ingredient>
    <ingredient num="4">1 cup ; cherry tomato, halved ; (200 g) </ingredient>
    <ingredient num="5">1 cup ; carrot, sliced ; (120 g) </ingredient>
    <ingredient num="6">1 teaspoon ; pepper </ingredient>
    <ingredient num="7">1 teaspoon ; salt </ingredient>
    <ingredient num="8">4 cups ; penne pasta ; (400 g) </ingredient>
    <ingredient num="9">1 cup ; parmesan cheese ; (110 g) </ingredient>
  </ingredients>
