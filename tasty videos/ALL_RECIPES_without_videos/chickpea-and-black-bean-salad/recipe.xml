<recipe>
  <title>Chickpea And Black Bean Salad Recipe by Tasty</title>
  <url>https://tasty.co/recipe/chickpea-and-black-bean-salad</url>
  <type>single</type>
  <ingredients>
    <ingredient num="0">2 cans ; black beans, drained and rinsed </ingredient>
    <ingredient num="1">2 cans ; chickpeas, drained and rinsed </ingredient>
    <ingredient num="2">½ ; red onion, chopped </ingredient>
    <ingredient num="3">20 ; cherry tomatoes, halved </ingredient>
    <ingredient num="4">½ cup ; fresh parsley, chopped ; (20 g) </ingredient>
    <ingredient num="5">½ cup ; fresh mint, chopped ; (15 g) </ingredient>
    <ingredient num="6">¼ cup ; olive oil ; (60 mL) </ingredient>
    <ingredient num="7">¼ cup ; apple cider vinegar ; (60 mL) </ingredient>
    <ingredient num="8">¼ cup ; lemon juice ; (60 mL) </ingredient>
    <ingredient num="9">salt, to taste </ingredient>
    <ingredient num="10">pepper, to taste </ingredient>
    <ingredient num="11">feta cheese, crumbled, to taste </ingredient>
  </ingredients>
