  <ingredients>
    <ingredient num="0">1 ; butternut squash </ingredient>
    <ingredient num="1">1 tablespoon ; olive oil </ingredient>
    <ingredient num="2">1 tablespoon ; dried rosemary </ingredient>
    <ingredient num="3">salt, to taste </ingredient>
    <ingredient num="4">pepper, to taste </ingredient>
  </ingredients>
