  <ingredients>
    <ingredient num="0">2 cups ; shredded parmesan cheese ; (200 g) </ingredient>
    <ingredient num="1">¼ cup ; pesto ; (55 g) </ingredient>
    <ingredient num="2">salt, to taste </ingredient>
    <ingredient num="3">pepper, to taste </ingredient>
  </ingredients>
  <categories>
    <category num="0">Low-Carb</category>
