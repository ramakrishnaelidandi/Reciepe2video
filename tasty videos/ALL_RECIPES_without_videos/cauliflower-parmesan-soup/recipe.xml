  <ingredients>
    <ingredient num="0">2 tablespoons ; butter </ingredient>
    <ingredient num="1">8 cloves ; garlic, minced </ingredient>
    <ingredient num="2">1 ; onion, chopped </ingredient>
    <ingredient num="3">2 heads ; cauliflower, riced </ingredient>
    <ingredient num="4">salt, to taste </ingredient>
    <ingredient num="5">pepper, to taste </ingredient>
    <ingredient num="6">8 cups ; vegetable broth ; (1.9 L) </ingredient>
    <ingredient num="7">2 cups ; water ; (470 mL) </ingredient>
    <ingredient num="8">2 sprigs ; fresh thyme </ingredient>
    <ingredient num="9">½ cup ; vegetarian parmesan cheese ; (55 g) </ingredient>
    <ingredient num="10">¼ cup ; fresh chives ; (10 g) </ingredient>
  </ingredients>
  <categories>
    <category num="0">Comfort Food</category>
    <category num="1">Vegetarian</category>
    <category num="2">Lunch</category>
    <category num="3">Weeknight</category>
  </categories>
