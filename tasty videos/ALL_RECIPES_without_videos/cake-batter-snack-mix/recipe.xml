  <ingredients>
    <ingredient num="0">1 teaspoon ; almond extract </ingredient>
    <ingredient num="1">5 cups ; rice cereal ; (235 g) </ingredient>
    <ingredient num="2">1 cup ; powdered sugar ; (160 g) </ingredient>
    <ingredient num="3">½ cup ; sprinkles ; (80 g) </ingredient>
    <ingredient num="4">1 ½ ; white chocolates ; 225 g </ingredient>
    <ingredient num="5">1 teaspoon ; coconut oil </ingredient>
  </ingredients>
