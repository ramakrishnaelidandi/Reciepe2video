  <ingredients>
    <ingredient num="0">1 bunch ; carrot, peeled and sliced </ingredient>
    <ingredient num="1">1 tablespoon ; cooking oil, of preference, or water </ingredient>
    <ingredient num="2">½ ; white onion, diced </ingredient>
    <ingredient num="3">½ teaspoon ; salt </ingredient>
    <ingredient num="4">½ teaspoon ; pepper </ingredient>
    <ingredient num="5">½ teaspoon ; garlic powder </ingredient>
    <ingredient num="6">1 teaspoon ; smoked paprika </ingredient>
    <ingredient num="7">fresh parsley, for garnish </ingredient>
  </ingredients>
