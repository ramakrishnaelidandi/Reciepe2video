  <ingredients>
    <ingredient num="0">1 cup ; shredded chicken, leftover ; (125 g) </ingredient>
    <ingredient num="1">8 oz ; cream cheese, softened ; (225 g) </ingredient>
    <ingredient num="2">½ cup ; ranch dressing ; (120 g) </ingredient>
    <ingredient num="3">½ cup ; buffalo sauce ; (120 g) </ingredient>
    <ingredient num="4">cheddar cheese </ingredient>
    <ingredient num="5">scallions, to serve </ingredient>
    <ingredient num="6">chip, to serve </ingredient>
  </ingredients>
