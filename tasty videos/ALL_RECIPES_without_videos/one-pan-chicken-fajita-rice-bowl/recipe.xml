  <ingredients>
    <ingredient num="0">1 ; red bell pepper, sliced </ingredient>
    <ingredient num="1">1 ; yellow bell pepper, sliced </ingredient>
    <ingredient num="2">½ ; white onion, sliced </ingredient>
    <ingredient num="3">2 ; large, boneless, skinless chicken breasts </ingredient>
    <ingredient num="4">2 tablespoons ; olive oil </ingredient>
    <ingredient num="5">4 cups ; wild rice, cooked, to serve ; (920 g) </ingredient>
    <ingredient num="6">½ ; avocado, sliced, optional </ingredient>
    <ingredient num="7">¼ teaspoon ; salt </ingredient>
    <ingredient num="8">¼ teaspoon ; pepper </ingredient>
    <ingredient num="9">¼ teaspoon ; cumin </ingredient>
    <ingredient num="10">½ teaspoon ; garlic powder </ingredient>
    <ingredient num="11">½ teaspoon ; paprika </ingredient>
  </ingredients>
