  <ingredients>
    <ingredient num="0">2 cups ; pancake mix ; (125 g) </ingredient>
    <ingredient num="1">½ cup ; butter, melted ; (115 g) </ingredient>
    <ingredient num="2">3 ; eggs, divided </ingredient>
    <ingredient num="3">½ tablespoon ; dried basil </ingredient>
    <ingredient num="4">½ tablespoon ; dried oregano </ingredient>
    <ingredient num="5">½ teaspoon ; garlic powder </ingredient>
    <ingredient num="6">⅓ cup ; mini pepperoni slice ; (50 g) </ingredient>
    <ingredient num="7">1 cup ; shredded mozzarella cheese ; (100 g) </ingredient>
    <ingredient num="8">1 cup ; milk, of choice ; (235 mL) </ingredient>
    <ingredient num="9">18 ; cherry tomatoes, or grape tomato </ingredient>
    <ingredient num="10">2 tablespoons ; heavy cream </ingredient>
    <ingredient num="11">2 tablespoons ; grated parmesan cheese </ingredient>
    <ingredient num="12">marinara sauce, for serving </ingredient>
  </ingredients>
  <categories>
    <category num="0">American</category>
    <category num="1">Appetizers</category>
    <category num="2">Kid-Friendly</category>
    <category num="3">Weeknight</category>
  </categories>
