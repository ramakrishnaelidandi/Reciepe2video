  <ingredients>
    <ingredient num="0">3 ; large zucchinis </ingredient>
    <ingredient num="1">2 ; eggs, beaten </ingredient>
    <ingredient num="2">2 cups ; italian breadcrumb ; (230 g) </ingredient>
    <ingredient num="3">1 teaspoon ; red pepper flakes </ingredient>
    <ingredient num="4">1 teaspoon ; salt </ingredient>
    <ingredient num="5">½ teaspoon ; pepper </ingredient>
  </ingredients>
