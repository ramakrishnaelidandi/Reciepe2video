<recipe>
  <title>Bacon Ranch Chicken Bake Recipe by Tasty</title>
  <url>https://tasty.co/recipe/bacon-ranch-chicken-bake</url>
  <type>single</type>
  <ingredients>
    <ingredient num="0">3 ; chicken breasts </ingredient>
    <ingredient num="1">salt, to taste </ingredient>
    <ingredient num="2">pepper, to taste </ingredient>
    <ingredient num="3">6 tablespoons ; ranch dressing </ingredient>
    <ingredient num="4">6 slices ; bacon </ingredient>
    <ingredient num="5">4 oz ; cheddar cheese ; (115 g) </ingredient>
  </ingredients>
  <categories>
    <category num="0">American</category>
    <category num="1">Comfort Food</category>
