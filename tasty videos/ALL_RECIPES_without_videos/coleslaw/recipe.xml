  <ingredients>
    <ingredient num="0">½ cup ; mayonnaise ; (120 g) </ingredient>
    <ingredient num="1">½ cup ; sour cream ; (115 g) </ingredient>
    <ingredient num="2">½ tablespoon ; lemon juice </ingredient>
    <ingredient num="3">2 tablespoons ; apple cider vinegar </ingredient>
    <ingredient num="4">2 teaspoons ; honey </ingredient>
    <ingredient num="5">1 teaspoon ; salt </ingredient>
    <ingredient num="6">½ teaspoon ; pepper </ingredient>
    <ingredient num="7">3 cups ; green cabbage, very thinly sliced, 1/2 small head ; (300 g) </ingredient>
    <ingredient num="8">3 cups ; purple cabbage, very thinly sliced, 1/2 small head ; (300 g) </ingredient>
    <ingredient num="9">1 ; large carrot, peeled and grated </ingredient>
  </ingredients>
