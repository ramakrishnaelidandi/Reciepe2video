  <ingredients>
    <ingredient num="0">15 oz ; chickpeas, 1 can, drained ; (400 g) </ingredient>
    <ingredient num="1">¼ cup ; olive oil ; (60 mL) </ingredient>
    <ingredient num="2">½ ; lemon, juiced </ingredient>
    <ingredient num="3">1 small clove ; garlic </ingredient>
    <ingredient num="4">¼ teaspoon ; ground cumin </ingredient>
    <ingredient num="5">½ teaspoon ; kosher salt </ingredient>
    <ingredient num="6">2 ; medium carrots </ingredient>
  </ingredients>
  <categories>
    <category num="0">Dairy-Free</category>
    <category num="1">Gluten-Free</category>
