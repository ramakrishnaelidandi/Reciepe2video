  <ingredients>
    <ingredient num="0">⅓ cup ; coconut oil, melted ; (70 g) </ingredient>
    <ingredient num="1">¼ cup ; maple syrup ; (55 g) </ingredient>
    <ingredient num="2">2 ; eggs </ingredient>
    <ingredient num="3">¼ cup ; milk ; (60 mL) </ingredient>
    <ingredient num="4">1 teaspoon ; vanilla extract </ingredient>
    <ingredient num="5">3 ; ripe bananas, mashed </ingredient>
    <ingredient num="6">2 cups ; whole wheat flour ; (230 g) </ingredient>
    <ingredient num="7">1 teaspoon ; cinnamon </ingredient>
    <ingredient num="8">½ teaspoon ; salt </ingredient>
    <ingredient num="9">1 teaspoon ; baking soda </ingredient>
  </ingredients>
