  <ingredients>
    <ingredient num="0">1 cup ; milk ; (235 mL) </ingredient>
    <ingredient num="1">½ cup ; maple syrup ; (110 g) </ingredient>
    <ingredient num="2">2 ; eggs </ingredient>
    <ingredient num="3">1 teaspoon ; vanilla extract </ingredient>
    <ingredient num="4">1 ½ cups ; quick-cook oats ; (150 g) </ingredient>
    <ingredient num="5">1 cup ; granola ; (120 g) </ingredient>
    <ingredient num="6">1 teaspoon ; baking powder </ingredient>
    <ingredient num="7">1 teaspoon ; cinnamon </ingredient>
    <ingredient num="8">1 cup ; mixed berries ; (100 g) </ingredient>
  </ingredients>
