<recipe>
  <title>Caramel Apple Crumble Cheesecake Recipe by Tasty</title>
  <url>https://tasty.co/recipe/caramel-apple-crumble-cheesecake</url>
  <type>single</type>
  <ingredients>
    <ingredient num="0">2 cups ; graham cracker, crumbled ; (170 g) </ingredient>
    <ingredient num="1">⅛ cup ; brown sugar ; (30 g) </ingredient>
    <ingredient num="2">⅓ cup ; butter ; (75 g) </ingredient>
    <ingredient num="3">16 oz ; cream cheese, softened ; (450 g) </ingredient>
    <ingredient num="4">¼ cup ; brown sugar ; (55 g) </ingredient>
    <ingredient num="5">¼ cup ; white sugar ; (50 g) </ingredient>
    <ingredient num="6">2 tablespoons ; cornstarch </ingredient>
    <ingredient num="7">1 teaspoon ; cinnamon </ingredient>
    <ingredient num="8">¼ teaspoon ; ginger </ingredient>
    <ingredient num="9">1 teaspoon ; vanilla extract </ingredient>
    <ingredient num="10">2 ; green apples, peeled and thinly sliced </ingredient>
    <ingredient num="11">¼ cup ; rolled oats ; (25 g) </ingredient>
    <ingredient num="12">¼ cup ; flour ; (30 g) </ingredient>
    <ingredient num="13">¼ cup ; brown sugar ; (55 g) </ingredient>
    <ingredient num="14">1 teaspoon ; cinnamon </ingredient>
    <ingredient num="15">2 tablespoons ; butter, melted </ingredient>
    <ingredient num="16">caramel sauce </ingredient>
  </ingredients>
  <categories>
    <category num="0">American</category>
    <category num="1">Vegetarian</category>
