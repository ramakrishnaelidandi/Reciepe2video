  <ingredients>
    <ingredient num="0">1 slice ; bread </ingredient>
    <ingredient num="1">1 ; egg </ingredient>
    <ingredient num="2">2 tablespoons ; milk </ingredient>
    <ingredient num="3">1 pinch ; cheddar cheese </ingredient>
    <ingredient num="4">salt, to taste </ingredient>
    <ingredient num="5">pepper, to taste </ingredient>
    <ingredient num="6">1 small handful ; spinach </ingredient>
    <ingredient num="7">red pepper, diced </ingredient>
    <ingredient num="8">tomato, diced </ingredient>
  </ingredients>
