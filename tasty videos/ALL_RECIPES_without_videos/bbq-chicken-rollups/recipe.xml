  <type>single</type>
  <ingredients>
    <ingredient num="0">1 ; pizza crust, refrigerated </ingredient>
    <ingredient num="1">½ cup ; BBQ sauce ; (145 g) </ingredient>
    <ingredient num="2">1 ½ cups ; rotisserie chicken, shredded ; (190 g) </ingredient>
    <ingredient num="3">½ g ; red onion, finely chopped ; ½ g </ingredient>
    <ingredient num="4">1 cup ; shredded cheese ; (100 g) </ingredient>
    <ingredient num="5">¼ cup ; parsley, chopped ; (10 g) </ingredient>
  </ingredients>
