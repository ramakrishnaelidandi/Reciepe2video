  <ingredients>
    <ingredient num="0">10 slices ; sandwich bread </ingredient>
    <ingredient num="1">sausage link, cooked </ingredient>
    <ingredient num="2">3 ; eggs </ingredient>
    <ingredient num="3">1 tablespoon ; milk </ingredient>
    <ingredient num="4">½ teaspoon ; cinnamon </ingredient>
    <ingredient num="5">½ teaspoon ; vanilla extract </ingredient>
  </ingredients>
