  <ingredients>
    <ingredient num="0">6 ; whole wheat pitas </ingredient>
    <ingredient num="1">2 tablespoons ; olive oil </ingredient>
    <ingredient num="2">½ teaspoon ; salt </ingredient>
    <ingredient num="3">½ teaspoon ; pepper </ingredient>
    <ingredient num="4">½ teaspoon ; garlic powder </ingredient>
  </ingredients>
