  <ingredients>
    <ingredient num="0">1 ; rotisserie chicken </ingredient>
    <ingredient num="1">1 cup ; marinara sauce ; (260 g) </ingredient>
    <ingredient num="2">6 slices ; bread, toasted </ingredient>
    <ingredient num="3">1 cup ; shredded mozzarella cheese ; (100 g) </ingredient>
  </ingredients>
