  <type>compilation</type>
  <comp_url>https://tasty.co/compilation/eggs-benedict-brunch-for-two</comp_url>
  <ingredients>
    <ingredient num="0">2 ; russet potatoes, peeled and grated </ingredient>
    <ingredient num="1">¼ cup ; butter, melted ; (50 mL) </ingredient>
    <ingredient num="2">½ cup ; shredded cheddar cheese ; (50 g) </ingredient>
    <ingredient num="3">1 ; egg </ingredient>
    <ingredient num="4">¼ cup ; chives, chopped ; (10 g) </ingredient>
    <ingredient num="5">1 teaspoon ; salt </ingredient>
    <ingredient num="6">½ teaspoon ; pepper </ingredient>
  </ingredients>
  <categories>
    <category num="0">American</category>
    <category num="1">Comfort Food</category>
    <category num="2">Brunch</category>
