    <ingredient num="2">6 ; carrots, diced </ingredient>
    <ingredient num="3">1 clove ; garlic </ingredient>
    <ingredient num="4">3 tablespoons ; butter </ingredient>
    <ingredient num="5">Â¼ cup ; flour ; (30 g) </ingredient>
    <ingredient num="6">2 cups ; chicken stock ; (470 mL) </ingredient>
    <ingredient num="7">2 cups ; half &amp; half ; (470 mL) </ingredient>
    <ingredient num="8">1 teaspoon ; pepper </ingredient>
    <ingredient num="9">1 teaspoon ; dried thyme </ingredient>
    <ingredient num="10">1 pinch ; ground nutmeg </ingredient>
    <ingredient num="11">1 cup ; frozen corn ; (175 g) </ingredient>
