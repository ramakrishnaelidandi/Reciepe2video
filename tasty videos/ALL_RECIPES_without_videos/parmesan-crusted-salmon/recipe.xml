  <ingredients>
    <ingredient num="0">6 oz ; skinless salmon ; (170 g) </ingredient>
    <ingredient num="1">7 oz ; asparagus ; (200 g) </ingredient>
    <ingredient num="2">olive oil, to taste </ingredient>
    <ingredient num="3">salt, to taste </ingredient>
    <ingredient num="4">pepper, to taste </ingredient>
    <ingredient num="5">1 ; egg </ingredient>
    <ingredient num="6">¼ cup ; panko breadcrumbs ; (12 g) </ingredient>
    <ingredient num="7">¼ cup ; grated parmesan cheese ; (30 g) </ingredient>
    <ingredient num="8">1 tablespoon ; fresh parsley, chopped </ingredient>
    <ingredient num="9">salt, to taste </ingredient>
  </ingredients>
