<recipe>
  <title>Curly Fries Recipe by Tasty</title>
  <url>https://tasty.co/recipe/curly-fries</url>
  <type>single</type>
  <ingredients>
    <ingredient num="0">2 ; russet potatoes </ingredient>
    <ingredient num="1">3 tablespoons ; canola oil </ingredient>
    <ingredient num="2">½ tablespoon ; salt </ingredient>
    <ingredient num="3">½ tablespoon ; pepper </ingredient>
    <ingredient num="4">½ tablespoon ; paprika </ingredient>
    <ingredient num="5">½ tablespoon ; cumin </ingredient>
    <ingredient num="6">½ tablespoon ; garlic powder </ingredient>
    <ingredient num="7">½ tablespoon ; chili powder </ingredient>
    <ingredient num="8">ketchup, to taste </ingredient>
  </ingredients>
