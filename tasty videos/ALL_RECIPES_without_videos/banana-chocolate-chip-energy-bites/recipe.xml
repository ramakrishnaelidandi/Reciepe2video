  <ingredients>
    <ingredient num="0">1 cup ; dry oat ; (100 g) </ingredient>
    <ingredient num="1">¼ cup ; almond butter ; (60 g) </ingredient>
    <ingredient num="2">¼ cup ; honey ; (85 g) </ingredient>
    <ingredient num="3">1 ; banana </ingredient>
    <ingredient num="4">½ cup ; chocolate chips ; (85 g) </ingredient>
  </ingredients>
