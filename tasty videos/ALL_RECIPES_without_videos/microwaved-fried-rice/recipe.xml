  <ingredients>
    <ingredient num="0">½ cup ; rice ; (100 g) </ingredient>
    <ingredient num="1">2 cups ; water ; (480 mL) </ingredient>
    <ingredient num="2">1 tablespoon ; soy sauce </ingredient>
    <ingredient num="3">1 teaspoon ; sesame oil </ingredient>
    <ingredient num="4">½ cup ; frozen vegetable ; (75 g) </ingredient>
    <ingredient num="5">2 ; eggs </ingredient>
    <ingredient num="6">salt, to taste </ingredient>
    <ingredient num="7">scallion, chopped, to serve </ingredient>
  </ingredients>
  <categories>
    <category num="0">American</category>
    <category num="1">Healthy</category>
