<recipe>
  <title>Garlic Parmesan Baked Carrot Fries Recipe by Tasty</title>
  <url>https://tasty.co/recipe/garlic-parmesan-baked-carrot-fries</url>
  <type>single</type>
  <ingredients>
    <ingredient num="0">2 ; carrots </ingredient>
    <ingredient num="1">1 tablespoon ; oil </ingredient>
    <ingredient num="2">¼ cup ; parmesan cheese ; (25 g) </ingredient>
    <ingredient num="3">1 tablespoon ; garlic powder </ingredient>
    <ingredient num="4">1 teaspoon ; pepper </ingredient>
    <ingredient num="5">¼ cup ; fresh parsley, chopped ; (10 g) </ingredient>
    <ingredient num="6">½ teaspoon ; salt </ingredient>
    <ingredient num="7">2 tablespoons ; greek yogurt </ingredient>
    <ingredient num="8">1 tablespoon ; lemon juice </ingredient>
    <ingredient num="9">½ teaspoon ; garlic salt </ingredient>
    <ingredient num="10">1 teaspoon ; pepper </ingredient>
  </ingredients>
  <categories>
    <category num="0">American</category>
    <category num="1">Healthy</category>
    <category num="2">Low-Carb</category>
    <category num="3">Vegetarian</category>
