  <ingredients>
    <ingredient num="0">16 oz ; refried bean, 1 can ; (450 g) </ingredient>
    <ingredient num="1">½ cup ; salsa ; (130 g) </ingredient>
    <ingredient num="2">½ cup ; sour cream ; (115 g) </ingredient>
    <ingredient num="3">1 cup ; guacamole ; (235 g) </ingredient>
    <ingredient num="4">lettuce </ingredient>
    <ingredient num="5">tomato </ingredient>
    <ingredient num="6">black olive </ingredient>
