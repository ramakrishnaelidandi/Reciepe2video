  <ingredients>
    <ingredient num="0">1 ½ cups ; vegan mayo ; (345 g) </ingredient>
    <ingredient num="1">3 tablespoons ; unsweetened non-dairy milk </ingredient>
    <ingredient num="2">2 teaspoons ; garlic powder </ingredient>
    <ingredient num="3">1 teaspoon ; onion powder </ingredient>
    <ingredient num="4">½ teaspoon ; salt </ingredient>
    <ingredient num="5">¼ teaspoon ; pepper </ingredient>
    <ingredient num="6">¼ teaspoon ; paprika </ingredient>
    <ingredient num="7">1 tablespoon ; apple cider vinegar </ingredient>
    <ingredient num="8">1 teaspoon ; lemon juice </ingredient>
    <ingredient num="9">2 teaspoons ; fresh parsley, chopped </ingredient>
    <ingredient num="10">1 teaspoon ; fresh dill, chopped </ingredient>
  </ingredients>
