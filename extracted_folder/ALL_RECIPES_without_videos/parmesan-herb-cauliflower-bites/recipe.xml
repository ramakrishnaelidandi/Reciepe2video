  <ingredients>
    <ingredient num="0">1 ; large head cauliflower </ingredient>
    <ingredient num="1">½ cup ; panko breadcrumbs ; (25 g) </ingredient>
    <ingredient num="2">½ cup ; parmesan cheese ; (55 g) </ingredient>
    <ingredient num="3">½ teaspoon ; salt </ingredient>
    <ingredient num="4">½ teaspoon ; fresh rosemary, minced </ingredient>
    <ingredient num="5">½ teaspoon ; fresh oregano, minced </ingredient>
    <ingredient num="6">½ teaspoon ; fresh thyme, minced </ingredient>
    <ingredient num="7">½ teaspoon ; fresh basil, minced </ingredient>
    <ingredient num="8">2 ; eggs </ingredient>
  </ingredients>
