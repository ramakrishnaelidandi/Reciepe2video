<recipe>
  <title>Sweet Potato Hash Cups Recipe by Tasty</title>
  <url>https://tasty.co/recipe/sweet-potato-hash-cups</url>
  <type>compilation</type>
  <comp_url>https://tasty.co/compilation/healthy-muffin-tin-breakfasts-4-ways</comp_url>
  <ingredients>
    <ingredient num="0">1 ; sweet potato, peeled </ingredient>
    <ingredient num="1">¼ cup ; panko ; (10 g) </ingredient>
    <ingredient num="2">2 tablespoons ; olive oil </ingredient>
    <ingredient num="3">1 teaspoon ; garlic powder </ingredient>
    <ingredient num="4">½ teaspoon ; salt </ingredient>
    <ingredient num="5">½ teaspoon ; pepper </ingredient>
    <ingredient num="6">broccoli, cut into small florets </ingredient>
    <ingredient num="7">shredded cheddar cheese </ingredient>
    <ingredient num="8">onion, diced </ingredient>
    <ingredient num="9">bell pepper, diced </ingredient>
    <ingredient num="10">shredded mozzarella cheese </ingredient>
    <ingredient num="11">egg </ingredient>
    <ingredient num="12">bacon, chopped </ingredient>
    <ingredient num="13">salt, to taste </ingredient>
    <ingredient num="14">pepper, to taste </ingredient>
  </ingredients>
  <categories>
    <category num="0">American</category>
    <category num="1">Brunch</category>
    <category num="2">Kid-Friendly</category>
