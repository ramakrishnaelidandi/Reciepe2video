  <ingredients>
    <ingredient num="0">1 tablespoon ; cooking oil, of preference </ingredient>
    <ingredient num="1">1 clove ; garlic, minced </ingredient>
    <ingredient num="2">¼ cup ; white onion, diced ; (35 g) </ingredient>
    <ingredient num="3">1 ; carrot, sliced </ingredient>
    <ingredient num="4">¼ cup ; frozen corn ; (45 g) </ingredient>
    <ingredient num="5">¼ cup ; frozen peas ; (35 g) </ingredient>
    <ingredient num="6">1 cup ; brown rice, cooked ; (230 g) </ingredient>
    <ingredient num="7">1 cup ; rotisserie chicken, shredded ; (125 g) </ingredient>
    <ingredient num="8">1 ; egg, scrambled </ingredient>
    <ingredient num="9">1 tablespoon ; soy sauce </ingredient>
    <ingredient num="10">½ tablespoon ; sesame oil </ingredient>
    <ingredient num="11">salt, to taste </ingredient>
    <ingredient num="12">pepper, to taste </ingredient>
    <ingredient num="13">green onion, for garnish </ingredient>
