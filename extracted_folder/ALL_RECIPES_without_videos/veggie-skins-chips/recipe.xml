    <ingredient num="2">3 ; potatoes, washed and skinned </ingredient>
    <ingredient num="3">2 tablespoons ; olive oil </ingredient>
    <ingredient num="4">2 teaspoons ; garlic powder </ingredient>
    <ingredient num="5">2 teaspoons ; onion powder </ingredient>
    <ingredient num="6">2 teaspoons ; paprika </ingredient>
