  <ingredients>
    <ingredient num="0">1 piece ; naan bread </ingredient>
    <ingredient num="1">baked beans, to taste </ingredient>
    <ingredient num="2">1 jar ; tikka masala sauce </ingredient>
    <ingredient num="3">cheddar cheese, to taste </ingredient>
    <ingredient num="4">1 ; egg </ingredient>
  </ingredients>
