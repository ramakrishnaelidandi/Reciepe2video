  <ingredients>
    <ingredient num="0">1 can ; black beans, drained and rinsed instead of flour </ingredient>
    <ingredient num="1">3 ; eggs </ingredient>
    <ingredient num="2">½ teaspoon ; baking powder </ingredient>
    <ingredient num="3">¼ cup ; unsweetened cocoa powder ; (30 g) </ingredient>
    <ingredient num="4">3 tablespoons ; coconut oil </ingredient>
    <ingredient num="5">½ cup ; maple syrup ; (110 g) </ingredient>
    <ingredient num="6">1 teaspoon ; vanilla extract </ingredient>
    <ingredient num="7">½ cup ; dark chocolate chip ; (85 g) </ingredient>
  </ingredients>
  <categories>
    <category num="0">Gluten-Free</category>
