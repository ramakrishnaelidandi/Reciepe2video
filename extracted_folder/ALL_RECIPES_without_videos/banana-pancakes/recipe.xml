  <ingredients>
    <ingredient num="0">2 ; large ripe bananas </ingredient>
    <ingredient num="1">2 ; eggs </ingredient>
    <ingredient num="2">1 teaspoon ; vanilla extract </ingredient>
    <ingredient num="3">1 teaspoon ; cinnamon </ingredient>
  </ingredients>
