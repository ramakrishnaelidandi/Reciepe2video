<recipe>
  <title>Strawberry Chocolate Mousse Recipe by Tasty</title>
  <url>https://tasty.co/recipe/strawberry-chocolate-mousse</url>
  <type>compilation</type>
  <comp_url>https://tasty.co/compilation/healthy-chicken-parmesan-dinner-for-2</comp_url>
  <ingredients>
    <ingredient num="0">6 oz ; dark chocolate, 72% is best ; (170 g) </ingredient>
    <ingredient num="1">½ cup ; low fat milk ; (120 mL) </ingredient>
    <ingredient num="2">1 teaspoon ; pure vanilla extract </ingredient>
    <ingredient num="3">1 pinch ; salt </ingredient>
    <ingredient num="4">1 cup ; greek yogurt ; (285 g) </ingredient>
    <ingredient num="5">8 ; strawberries </ingredient>
  </ingredients>
  <categories>
    <category num="0">American</category>
    <category num="1">Gluten-Free</category>
    <category num="2">Healthy</category>
    <category num="3">Vegetarian</category>
