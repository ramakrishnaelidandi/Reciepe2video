  <ingredients>
    <ingredient num="0">1 ; banana </ingredient>
    <ingredient num="1">1 ; egg </ingredient>
    <ingredient num="2">1 teaspoon ; vanilla extract </ingredient>
    <ingredient num="3">½ cup ; almond milk ; (120 mL) </ingredient>
    <ingredient num="4">¼ cup ; maple syrup ; (55 g) </ingredient>
    <ingredient num="5">½ cup ; greek yogurt ; (140 g) </ingredient>
    <ingredient num="6">1 ½ cups ; rolled oats ; (150 g) </ingredient>
    <ingredient num="7">1 teaspoon ; baking powder </ingredient>
    <ingredient num="8">1 teaspoon ; cinnamon </ingredient>
    <ingredient num="9">salt, to taste </ingredient>
    <ingredient num="10">strawberry, sliced </ingredient>
    <ingredient num="11">blueberry </ingredient>
    <ingredient num="12">apple, sliced </ingredient>
    <ingredient num="13">dark chocolate chip </ingredient>
    <ingredient num="14">nut, chopped </ingredient>
    <ingredient num="15">dried cranberry </ingredient>
  </ingredients>
