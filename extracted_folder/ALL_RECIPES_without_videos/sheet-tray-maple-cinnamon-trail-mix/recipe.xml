<recipe>
  <title>Sheet Tray Maple Cinnamon Trail Mix Recipe by Tasty</title>
  <url>https://tasty.co/recipe/sheet-tray-maple-cinnamon-trail-mix</url>
  <type>compilation</type>
  <comp_url>https://tasty.co/compilation/3-easy-sheet-tray-snacks</comp_url>
  <ingredients>
    <ingredient num="0">1 cup ; pumpkin seeds ; (130 g) </ingredient>
    <ingredient num="1">1 cup ; walnuts, chopped ; (100 g) </ingredient>
    <ingredient num="2">1 cup ; raw almond, whole, unsalted ; (140 g) </ingredient>
    <ingredient num="3">1 teaspoon ; ground cinnamon </ingredient>
    <ingredient num="4">3 tablespoons ; maple syrup </ingredient>
    <ingredient num="5">1 ½ teaspoons ; salt </ingredient>
    <ingredient num="6">1 cup ; dried cranberry ; (130 g) </ingredient>
  </ingredients>
  <categories>
    <category num="0">Dairy-Free</category>
    <category num="1">Healthy</category>
    <category num="2">Vegan</category>
    <category num="3">Vegetarian</category>
    <category num="4">Snacks</category>
  </categories>
