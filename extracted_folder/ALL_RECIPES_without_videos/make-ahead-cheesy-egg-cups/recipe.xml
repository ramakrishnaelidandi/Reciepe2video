  <ingredients>
    <ingredient num="0">15 oz ; frozen hash brown, thawed ; (425 g) </ingredient>
    <ingredient num="1">1 ½ cups ; shredded cheddar cheese ; (150 g) </ingredient>
    <ingredient num="2">1 teaspoon ; salt </ingredient>
    <ingredient num="3">½ teaspoon ; pepper </ingredient>
    <ingredient num="4">1 tablespoon ; oil </ingredient>
    <ingredient num="5">8 ; eggs </ingredient>
    <ingredient num="6">3 slices ; bacon, cooked, crumbled </ingredient>
    <ingredient num="7">salt, to taste </ingredient>
    <ingredient num="8">pepper, to taste </ingredient>
  </ingredients>
