<recipe>
  <title>Vegan Buffalo Chickpea Dip Recipe by Tasty</title>
  <url>https://tasty.co/recipe/vegan-buffalo-chickpea-dip</url>
  <type>compilation</type>
  <comp_url>https://tasty.co/compilation/4-vegan-dipping-sauces</comp_url>
  <ingredients>
    <ingredient num="0">15 oz ; cannellini bean, 1 can, drained and rinsed ; (425 g) </ingredient>
    <ingredient num="1">1 cup ; raw cashew ; (130 g) </ingredient>
    <ingredient num="2">¾ cup ; hot sauce ; (180 mL) </ingredient>
    <ingredient num="3">½ cup ; non-dairy milk ; (120 mL) </ingredient>
    <ingredient num="4">½ ; lemon, juiced </ingredient>
    <ingredient num="5">1 tablespoon ; nutritional yeast </ingredient>
    <ingredient num="6">1 teaspoon ; paprika </ingredient>
    <ingredient num="7">½ teaspoon ; onion powder </ingredient>
    <ingredient num="8">½ teaspoon ; garlic powder </ingredient>
    <ingredient num="9">1 teaspoon ; salt </ingredient>
    <ingredient num="10">½ teaspoon ; pepper </ingredient>
    <ingredient num="11">15 oz ; chickpeas, 1 can, drained and rinsed ; (425 g) </ingredient>
    <ingredient num="12">1 cup ; shredded vegan cheddar cheese ; (100 g) </ingredient>
    <ingredient num="13">fresh scallion, for garnish </ingredient>
    <ingredient num="14">fresh parsley, for garnish </ingredient>
  </ingredients>
  <categories>
    <category num="0">Dairy-Free</category>
    <category num="1">Vegan</category>
    <category num="2">Vegetarian</category>
    <category num="3">Appetizers</category>
    <category num="4">Snacks</category>
  </categories>
