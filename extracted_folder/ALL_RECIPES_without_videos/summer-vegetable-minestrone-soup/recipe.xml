<recipe>
  <title>Summer Vegetable Minestrone Soup Recipe by Tasty</title>
  <url>https://tasty.co/recipe/summer-vegetable-minestrone-soup</url>
  <type>single</type>
  <ingredients>
    <ingredient num="0">1 tablespoon ; oil </ingredient>
    <ingredient num="1">2 ; carrots, chopped </ingredient>
    <ingredient num="2">2 stalks ; celery, chopped </ingredient>
    <ingredient num="3">1 ; onion, chopped </ingredient>
    <ingredient num="4">1 teaspoon ; salt </ingredient>
    <ingredient num="5">Â½ teaspoon ; black pepper </ingredient>
    <ingredient num="6">1 ; potato, peeled and cubed </ingredient>
    <ingredient num="7">3 cloves ; garlic, minced </ingredient>
    <ingredient num="8">28 oz ; diced tomato, 1 can ; (795 g) </ingredient>
    <ingredient num="9">8 cups ; vegetable stock ; (1.8 L) </ingredient>
    <ingredient num="10">2 ; zucchinis, diced </ingredient>
    <ingredient num="11">15 oz ; cannellini bean, 1 can, rinsed and drained ; (425 g) </ingredient>
    <ingredient num="12">3 cups ; spinach ; (120 g) </ingredient>
    <ingredient num="13">2 cups ; elbow pasta ; (200 g) </ingredient>
  </ingredients>
  <categories>
    <category num="0">American</category>
