  <ingredients>
    <ingredient num="0">4 ; egg whites </ingredient>
    <ingredient num="1">1 teaspoon ; garlic powder </ingredient>
    <ingredient num="2">2 teaspoons ; parsley </ingredient>
    <ingredient num="3">salt, to taste </ingredient>
    <ingredient num="4">pepper, to taste </ingredient>
    <ingredient num="5">½ cup ; shredded cheese blend ; (50 g) </ingredient>
  </ingredients>
  <categories>
    <category num="0">Low-Carb</category>
    <category num="1">Vegetarian</category>
    <category num="2">Kid-Friendly</category>
