  <type>single</type>
  <ingredients>
    <ingredient num="0">12 oz ; chicken, shredded ; (340 g) </ingredient>
    <ingredient num="1">⅔ cup ; fat free plain greek yogurt, or low-fat ; (190 g) </ingredient>
    <ingredient num="2">½ cup ; celery, finely chopped ; (115 g) </ingredient>
    <ingredient num="3">1 ½ tablespoons ; sweet relish </ingredient>
    <ingredient num="4">1 tablespoon ; dijon mustard </ingredient>
    <ingredient num="5">1 ; scallion, thinly sliced </ingredient>
    <ingredient num="6">1 teaspoon ; garlic powder </ingredient>
    <ingredient num="7">½ teaspoon ; paprika </ingredient>
    <ingredient num="8">salt, to taste </ingredient>
    <ingredient num="9">pepper, to taste </ingredient>
    <ingredient num="10">4 leaves ; lettuce </ingredient>
    <ingredient num="11">20 ; cherry tomatoes </ingredient>
    <ingredient num="12">4 ; pita breads </ingredient>
  </ingredients>
  <categories>
    <category num="0">American</category>
    <category num="1">Greek</category>
