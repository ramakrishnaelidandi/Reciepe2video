  <ingredients>
    <ingredient num="0">½ ; cucumber, diced </ingredient>
    <ingredient num="1">½ ; eggplant, diced </ingredient>
    <ingredient num="2">2 ; okras, chopped </ingredient>
    <ingredient num="3">1 ; japanese ginger, chopped </ingredient>
    <ingredient num="4">2 ; shiso leaves, chopped </ingredient>
    <ingredient num="5">2 tablespoons ; soy sauce </ingredient>
    <ingredient num="6">2 teaspoons ; mirin </ingredient>
    <ingredient num="7">1 tablespoon ; vinegar </ingredient>
    <ingredient num="8">¼ teaspoon ; sugar </ingredient>
    <ingredient num="9">4 oz ; ground chicken ; (115 g) </ingredient>
    <ingredient num="10">2 tablespoons ; sake </ingredient>
    <ingredient num="11">salt, to taste </ingredient>
    <ingredient num="12">black pepper, to taste </ingredient>
    <ingredient num="13">1 teaspoon ; green onion, thinly sliced </ingredient>
    <ingredient num="14">4 ; tortillas </ingredient>
    <ingredient num="15">lettuce, to serve </ingredient>
    <ingredient num="16">4 ; cherry tomatoes, quartered </ingredient>
    <ingredient num="17">shredded mozzarella cheese, to serve </ingredient>
  </ingredients>
  <categories>
    <category num="0">Japanese</category>
    <category num="1">Healthy</category>
    <category num="2">Dinner</category>
  </categories>
  <video>
