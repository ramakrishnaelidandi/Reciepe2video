  <ingredients>
    <ingredient num="0">1 tablespoon ; oil </ingredient>
    <ingredient num="1">1 ; onion, chopped </ingredient>
    <ingredient num="2">1 stalk ; celery, chopped </ingredient>
    <ingredient num="3">3 cloves ; garlic, minced </ingredient>
    <ingredient num="4">1 teaspoon ; salt </ingredient>
    <ingredient num="5">¼ teaspoon ; pepper </ingredient>
    <ingredient num="6">2 tablespoons ; tomato paste </ingredient>
    <ingredient num="7">¼ cup ; fresh basil ; (10 g) </ingredient>
    <ingredient num="8">28 oz ; crushed tomato, 2 cans ; (795 g) </ingredient>
    <ingredient num="9">6 cups ; vegetable broth ; (1 ½ L) </ingredient>
    <ingredient num="10">½ teaspoon ; baking soda </ingredient>
    <ingredient num="11">2 sprigs ; fresh thyme </ingredient>
  </ingredients>
