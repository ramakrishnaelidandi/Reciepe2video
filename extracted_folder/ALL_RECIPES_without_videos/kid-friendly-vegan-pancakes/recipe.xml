<recipe>
  <title>Kid-Friendly Vegan Pancakes Recipe by Tasty</title>
  <url>https://tasty.co/recipe/kid-friendly-vegan-pancakes</url>
  <type>compilation</type>
  <comp_url>https://tasty.co/compilation/5-kid-friendly-vegan-meals</comp_url>
  <ingredients>
    <ingredient num="0">2 cups ; flour ; (250 g) </ingredient>
    <ingredient num="1">2 tablespoons ; sugar </ingredient>
    <ingredient num="2">2 tablespoons ; baking powder </ingredient>
    <ingredient num="3">½ teaspoon ; salt </ingredient>
    <ingredient num="4">2 cups ; almond milk ; (480 mL) </ingredient>
    <ingredient num="5">2 tablespoons ; apple cider vinegar </ingredient>
    <ingredient num="6">2 teaspoons ; vanilla </ingredient>
    <ingredient num="7">vegan chocolate sandwich cookie </ingredient>
    <ingredient num="8">strawberry </ingredient>
    <ingredient num="9">banana </ingredient>
    <ingredient num="10">blueberry </ingredient>
    <ingredient num="11">blackberry </ingredient>
  </ingredients>
  <categories>
    <category num="0">Healthy</category>
    <category num="1">Vegan</category>
    <category num="2">Breakfast</category>
    <category num="3">Brunch</category>
  </categories>
