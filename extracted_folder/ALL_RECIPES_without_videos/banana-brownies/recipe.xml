  <ingredients>
    <ingredient num="0">1 ; banana </ingredient>
    <ingredient num="1">1 box ; brownie mix </ingredient>
    <ingredient num="2">¼ cup ; water ; (60 mL) </ingredient>
    <ingredient num="3">2 ; eggs </ingredient>
  </ingredients>
  <categories>
