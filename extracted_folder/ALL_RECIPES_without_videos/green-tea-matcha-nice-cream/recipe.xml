  <ingredients>
    <ingredient num="0">3 ; ripe bananas </ingredient>
    <ingredient num="1">⅓ cup ; almond milk ; (80 mL) </ingredient>
    <ingredient num="2">½ tablespoon ; matcha powder </ingredient>
    <ingredient num="3">1 teaspoon ; agave </ingredient>
  </ingredients>
  <categories>
    <category num="0">Healthy</category>
    <category num="1">Vegan</category>
    <category num="2">Vegetarian</category>
  </categories>
