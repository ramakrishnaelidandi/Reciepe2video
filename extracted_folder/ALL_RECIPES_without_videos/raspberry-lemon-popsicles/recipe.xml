  <ingredients>
    <ingredient num="0">1 cup ; raspberry ; (125 g) </ingredient>
    <ingredient num="1">1 cup ; fresh lemon juice ; (240 mL) </ingredient>
    <ingredient num="2">1 ½ cups ; water ; (360 mL) </ingredient>
    <ingredient num="3">popsicle mold, with 6 slots </ingredient>
  </ingredients>
