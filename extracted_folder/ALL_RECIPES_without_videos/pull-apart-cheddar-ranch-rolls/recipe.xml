<recipe>
  <title>Pull-Apart Cheddar Ranch Rolls Recipe by Tasty</title>
  <url>https://tasty.co/recipe/pull-apart-cheddar-ranch-rolls</url>
  <type>single</type>
  <ingredients>
    <ingredient num="0">1 can ; premade biscuit </ingredient>
    <ingredient num="1">1 cup ; shredded cheddar cheese ; (100 g) </ingredient>
    <ingredient num="2">¼ cup ; chives, chopped ; (10 g) </ingredient>
    <ingredient num="3">1 tablespoon ; ranch seasoning </ingredient>
    <ingredient num="4">3 tablespoons ; butter, melted </ingredient>
  </ingredients>
  <categories>
    <category num="0">American</category>
    <category num="1">Comfort Food</category>
    <category num="2">Vegetarian</category>
    <category num="3">Kid-Friendly</category>
