<recipe>
  <title>Skillet Chicken Pot Pie Recipe by Tasty</title>
  <url>https://tasty.co/recipe/skillet-chicken-pot-pie</url>
  <type>compilation</type>
  <comp_url>https://tasty.co/compilation/4-easy-rotisserie-chicken-dinners</comp_url>
  <ingredients>
    <ingredient num="0">2 ½ tablespoons ; butter </ingredient>
    <ingredient num="1">3 tablespoons ; flour </ingredient>
    <ingredient num="2">1 ½ cups ; milk ; (355 mL) </ingredient>
    <ingredient num="3">2 ; rotisserie chicken breasts, shredded </ingredient>
    <ingredient num="4">½ cup ; frozen peas ; (75 g) </ingredient>
    <ingredient num="5">½ cup ; frozen carrot ; (60 g) </ingredient>
    <ingredient num="6">salt, to taste </ingredient>
    <ingredient num="7">pepper, to taste </ingredient>
    <ingredient num="8">1 ; small potato, peeled and chopped </ingredient>
    <ingredient num="9">½ cup ; cream ; (120 mL) </ingredient>
    <ingredient num="10">1 ; premade pie crust </ingredient>
  </ingredients>
  <categories>
    <category num="0">American</category>
    <category num="1">Comfort Food</category>
    <category num="2">Dinner</category>
    <category num="3">Kid-Friendly</category>
