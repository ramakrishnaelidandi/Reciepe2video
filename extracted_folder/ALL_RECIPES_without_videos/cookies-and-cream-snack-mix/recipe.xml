  <ingredients>
    <ingredient num="0">1 teaspoon ; vanilla extract </ingredient>
    <ingredient num="1">5 cups ; rice cereal ; (235 g) </ingredient>
    <ingredient num="2">1 cup ; powdered sugar ; (160 g) </ingredient>
    <ingredient num="3">½ cup ; crushed cookie ; (40 g) </ingredient>
    <ingredient num="4">1 ½ cups ; white chocolate ; (255 g) </ingredient>
    <ingredient num="5">1 teaspoon ; coconut oil </ingredient>
  </ingredients>
