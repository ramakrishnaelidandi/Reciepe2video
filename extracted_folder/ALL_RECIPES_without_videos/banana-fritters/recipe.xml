  <ingredients>
    <ingredient num="0">2 ; ripe bananas, peeled </ingredient>
    <ingredient num="1">1 cup ; pancake mix ; (125 g) </ingredient>
    <ingredient num="2">½ teaspoon ; cinnamon </ingredient>
    <ingredient num="3">¾ cup ; milk ; (175 mL) </ingredient>
    <ingredient num="4">oil, for frying </ingredient>
    <ingredient num="5">powdered sugar, to serve </ingredient>
  </ingredients>
