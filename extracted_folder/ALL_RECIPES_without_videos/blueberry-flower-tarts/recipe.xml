<recipe>
  <title>Blueberry Flower Tarts Recipe by Tasty</title>
  <url>https://tasty.co/recipe/blueberry-flower-tarts</url>
  <type>compilation</type>
  <comp_url>https://tasty.co/compilation/4-pie-crust-desserts</comp_url>
  <ingredients>
    <ingredient num="0">2 ; refrigerated pie crusts </ingredient>
    <ingredient num="1">2 cups ; blueberry ; (200 g) </ingredient>
    <ingredient num="2">1 tablespoon ; flour </ingredient>
    <ingredient num="3">1 teaspoon ; cornstarch </ingredient>
    <ingredient num="4">3 tablespoons ; sugar </ingredient>
    <ingredient num="5">1 teaspoon ; lemon zest </ingredient>
    <ingredient num="6">1 tablespoon ; unsalted butter, softened </ingredient>
    <ingredient num="7">egg wash </ingredient>
    <ingredient num="8">powdered sugar, to serve </ingredient>
  </ingredients>
  <categories>
    <category num="0">Desserts</category>
    <category num="1">Indulgent Sweets</category>
    <category num="2">Bakery Goods</category>
  </categories>
