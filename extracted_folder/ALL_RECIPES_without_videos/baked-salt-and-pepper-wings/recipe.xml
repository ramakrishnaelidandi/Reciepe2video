  <ingredients>
    <ingredient num="0">2 lb ; chicken wings ; (905 g) </ingredient>
    <ingredient num="1">2 tablespoons ; olive oil </ingredient>
    <ingredient num="2">2 tablespoons ; cornstarch </ingredient>
    <ingredient num="3">1 teaspoon ; pepper </ingredient>
    <ingredient num="4">2 teaspoons ; salt </ingredient>
  </ingredients>
