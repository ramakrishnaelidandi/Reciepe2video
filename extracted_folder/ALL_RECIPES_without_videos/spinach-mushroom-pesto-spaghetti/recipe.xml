  <ingredients>
    <ingredient num="0">1 tablespoon ; canola oil </ingredient>
    <ingredient num="1">5 oz ; spinach ; (140 g) </ingredient>
    <ingredient num="2">2 cups ; mushroom, sliced ; (150 g) </ingredient>
    <ingredient num="3">1 teaspoon ; salt </ingredient>
    <ingredient num="4">1 teaspoon ; pepper </ingredient>
    <ingredient num="5">1 cup ; pesto ; (225 g) </ingredient>
    <ingredient num="6">½ cup ; parmesan cheese ; (110 g) </ingredient>
    <ingredient num="7">½ lb ; spaghetti, cooked ; (225 g) </ingredient>
