  <ingredients>
    <ingredient num="0">3 ; ripe bananas, mashed </ingredient>
    <ingredient num="1">¾ cup ; honey ; (255 g) </ingredient>
    <ingredient num="2">1 ; egg </ingredient>
    <ingredient num="3">2 tablespoons ; almond milk </ingredient>
    <ingredient num="4">1 teaspoon ; vanilla extract </ingredient>
    <ingredient num="5">2 cups ; whole wheat flour ; (260 g) </ingredient>
    <ingredient num="6">1 teaspoon ; baking soda </ingredient>
    <ingredient num="7">salt, a pinch </ingredient>
    <ingredient num="8">¼ cup ; applesauce, or Greek yogurt ; (65 mL) </ingredient>
    <ingredient num="9">¼ cup ; greek yogurt ; (70 g) </ingredient>
  </ingredients>
  <categories>
    <category num="0">Healthy</category>
    <category num="1">Vegetarian</category>
    <category num="2">Desserts</category>
  </categories>
