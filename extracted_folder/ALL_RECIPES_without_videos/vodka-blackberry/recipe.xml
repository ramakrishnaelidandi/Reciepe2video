  <ingredients>
    <ingredient num="0">½ cup ; ice ; (110 g) </ingredient>
    <ingredient num="1">1 tablespoon ; blackberry jam </ingredient>
    <ingredient num="2">2 tablespoons ; lemon juice </ingredient>
    <ingredient num="3">¼ cup ; vodka ; (60 mL) </ingredient>
    <ingredient num="4">lemon peel, for garnish </ingredient>
    <ingredient num="5">cocktail shaker </ingredient>
  </ingredients>
