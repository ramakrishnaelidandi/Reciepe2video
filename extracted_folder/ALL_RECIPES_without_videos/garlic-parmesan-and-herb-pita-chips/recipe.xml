  <ingredients>
    <ingredient num="0">1 ; pita bread </ingredient>
    <ingredient num="1">2 tablespoons ; olive oil </ingredient>
    <ingredient num="2">1 teaspoon ; vegetarian parmesan cheese </ingredient>
    <ingredient num="3">1 teaspoon ; garlic, minced </ingredient>
    <ingredient num="4">1 teaspoon ; fresh parsley, chopped </ingredient>
  </ingredients>
