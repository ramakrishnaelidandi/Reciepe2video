  <ingredients>
    <ingredient num="0">2 cups ; shredded chicken ; (250 g) </ingredient>
    <ingredient num="1">1 cup ; black beans ; (170 g) </ingredient>
    <ingredient num="2">1 cup ; corn ; (175 g) </ingredient>
    <ingredient num="3">1 jar ; salsa </ingredient>
    <ingredient num="4">1 cup ; shredded cheese blend ; (100 g) </ingredient>
    <ingredient num="5">4 ; large flour tortillas </ingredient>
    <ingredient num="6">sour cream </ingredient>
    <ingredient num="7">guacamole </ingredient>
    <ingredient num="8">salsa </ingredient>
  </ingredients>
