  <ingredients>
    <ingredient num="0">½ loaf ; whole wheat bread </ingredient>
    <ingredient num="1">1 ½ cups ; frozen blueberry ; (150 g) </ingredient>
    <ingredient num="2">6 ; eggs </ingredient>
    <ingredient num="3">1 tablespoon ; maple syrup </ingredient>
    <ingredient num="4">2 tablespoons ; coconut oil, melted </ingredient>
    <ingredient num="5">1 tablespoon ; vanilla extract </ingredient>
    <ingredient num="6">1 teaspoon ; cinnamon </ingredient>
    <ingredient num="7">1 ; lemon, zested </ingredient>
    <ingredient num="8">2 cups ; milk, or milk alternative ; (470 mL) </ingredient>
    <ingredient num="9">maple syrup, to serve </ingredient>
    <ingredient num="10">powdered sugar, to serve </ingredient>
  </ingredients>
