  <ingredients>
    <ingredient num="0">3 ; gold potatoes </ingredient>
    <ingredient num="1">1 tablespoon ; cooking oil </ingredient>
    <ingredient num="2">2 cups ; cheese cracker, cheddar cheese ; (200 g) </ingredient>
    <ingredient num="3">1 tablespoon ; salt </ingredient>
  </ingredients>
