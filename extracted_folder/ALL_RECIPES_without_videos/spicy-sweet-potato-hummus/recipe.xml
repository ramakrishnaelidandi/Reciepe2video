<recipe>
  <title>Spicy Sweet Potato Hummus Recipe by Tasty</title>
  <url>https://tasty.co/recipe/spicy-sweet-potato-hummus</url>
  <type>compilation</type>
  <comp_url>https://tasty.co/compilation/hummus-3-ways</comp_url>
  <ingredients>
    <ingredient num="0">1 ; sweet potato, peeled and cooked </ingredient>
    <ingredient num="1">15 oz ; garbanzo beans, 1 can ; (425 g) </ingredient>
    <ingredient num="2">3 tablespoons ; tahini </ingredient>
    <ingredient num="3">1 ; lemon, juiced </ingredient>
    <ingredient num="4">2 cloves ; garlic </ingredient>
    <ingredient num="5">salt, to taste </ingredient>
    <ingredient num="6">¼ teaspoon ; black pepper </ingredient>
    <ingredient num="7">1 ½ teaspoons ; cayenne pepper </ingredient>
    <ingredient num="8">¼ teaspoon ; cumin </ingredient>
    <ingredient num="9">½ teaspoon ; paprika </ingredient>
    <ingredient num="10">3 tablespoons ; olive oil </ingredient>
    <ingredient num="11">pine nuts, to garnish </ingredient>
    <ingredient num="12">3 tablespoons ; water </ingredient>
  </ingredients>
