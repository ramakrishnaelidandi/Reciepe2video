  <ingredients>
    <ingredient num="0">1 head ; cauliflower, divided into florets </ingredient>
    <ingredient num="1">1 tablespoon ; olive oil </ingredient>
    <ingredient num="2">2 teaspoons ; cumin </ingredient>
    <ingredient num="3">1 teaspoon ; turmeric </ingredient>
    <ingredient num="4">½ teaspoon ; chili powder </ingredient>
    <ingredient num="5">1 teaspoon ; salt </ingredient>
  </ingredients>
