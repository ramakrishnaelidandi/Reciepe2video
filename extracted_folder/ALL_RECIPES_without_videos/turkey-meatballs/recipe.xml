  <ingredients>
    <ingredient num="0">1 lb ; lean ground turkey ; (455 g) </ingredient>
    <ingredient num="1">½ cup ; italian breadcrumbs ; (60 g) </ingredient>
    <ingredient num="2">1 ; egg, beaten </ingredient>
    <ingredient num="3">½ teaspoon ; salt </ingredient>
    <ingredient num="4">½ teaspoon ; pepper </ingredient>
    <ingredient num="5">1 ½ tablespoons ; olive oil </ingredient>
  </ingredients>
