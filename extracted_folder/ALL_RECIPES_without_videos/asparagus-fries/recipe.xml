  <ingredients>
    <ingredient num="0">1 bundle ; asparagus </ingredient>
    <ingredient num="1">1 cup ; almond, chopped ; (140 g) </ingredient>
    <ingredient num="2">⅓ cup ; parmesan cheese ; (35 g) </ingredient>
    <ingredient num="3">1 tablespoon ; garlic powder </ingredient>
    <ingredient num="4">2 tablespoons ; dried oregano </ingredient>
    <ingredient num="5">1 teaspoon ; salt </ingredient>
    <ingredient num="6">1 teaspoon ; pepper </ingredient>
    <ingredient num="7">2 ; eggs </ingredient>
    <ingredient num="8">1 cup ; plain greek yogurt ; (285 g) </ingredient>
    <ingredient num="9">1 tablespoon ; lemon juice </ingredient>
    <ingredient num="10">2 tablespoons ; chives </ingredient>
    <ingredient num="11">¼ teaspoon ; salt </ingredient>
    <ingredient num="12">¼ teaspoon ; pepper </ingredient>
  </ingredients>
